digraph simple {
  layout="neato"
  Ready [pos="2,1!"];
  Busy [pos="0,1!"];
  Error [pos="1,0!"];
  Disabled [pos="2,2!"];
  Ready -> Busy [label="start"];
  Busy -> Ready [label="stop"];
  Busy -> Ready [label="finish", color=blue];
  Busy -> Error [color=blue];
  Error -> Ready [label="reset"];
  Ready -> Disabled [label="disable"];
  Disabled -> Ready [label="enable"];
}
